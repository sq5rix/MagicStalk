#:kivy 1.0.9


<RootWidget>:

    obj_cur_mst: cur_mst
    obj_avg_mst: avg_mst
    obj_cur_temp: cur_temp
    obj_adj_mst: adj_mst

    GridLayout:
        cols: 2
        Label:
            text: 'Current Moisture'
            font_size: self.height / 4
        Label:
            id: cur_mst
            text: '825'
            font_size: self.height / 4
        Label:
            text: 'Average Moisture'
            font_size: self.height / 4
        Label:
            id: avg_mst
            text: '900'
            font_size: self.height / 4
        Label:
            id: cur_temp
            text: 'Temperature'
            font_size: self.height / 4
        Label:
            id: cur_tmp
            text: '25'
            font_size: self.height / 4
        Label:
            text: 'Adjusted Moisture'
            font_size: self.height / 4
        Label:
            id: adj_mst
            text: '1000'
            font_size: self.height / 4
        Label:
            text: 'USB Port'
            font_size: self.height / 4
        Label:
            id: usb_port
            text: 'USB1'
            font_size: self.height / 4
